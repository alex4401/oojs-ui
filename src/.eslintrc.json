{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/jsduck"
	],
	"plugins": [
		"mediawiki"
	],
	"globals": {
		"OO": "writable",
		"Set": "readonly"
	},
	"rules": {
		"mediawiki/msg-doc": "error",
		"no-jquery/no-visibility": "error",
		"no-var": "off",
		"es-x/no-regexp-prototype-flags": "off"
	}
}
